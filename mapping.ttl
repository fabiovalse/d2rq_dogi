  d2rq:propertyName rdfs:domain;
  d2rq:propertyValue bibo:Article;
  .
map:bibliographyType_range a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:range;
  d2rq:propertyValue dogi:BibliographyType;
  .
