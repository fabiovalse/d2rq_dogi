  d2rq:join "tabRifBibl.IDRivista = tabRiviste.IDRivista";
  d2rq:condition "tabDocumenti.PagFine = 0";
  d2rq:condition "LEFT(tabRiviste.IDRivista, 1) = '9'";
  .
map:tabDocumenti_PagFine a d2rq:PropertyBridge;
