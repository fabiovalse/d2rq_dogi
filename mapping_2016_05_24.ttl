### Table tabClassificazioni
###
map:tabClassificazioni a d2rq:ClassMap;
  d2rq:dataStorage map:database;
  d2rq:uriSqlExpression "CONCAT('http://www.dogi.cnr.it/resource/Classification/', CONCAT(tabClassificazioni.NS, (CASE WHEN (tabClassificazioni.ND != '') THEN CONCAT('_', tabClassificazioni.ND) ELSE '' END)))";
  d2rq:classDefinitionLabel "Classification"@en;
  d2rq:classDefinitionComment "An entry of the classification scheme for law, specifically developed for the DoGi database. It could be a pair sistematico-descrittore or a sistematico."@en;
  d2rq:additionalClassDefinitionProperty map:classification_subClassOf;
  d2rq:class dogi:Classification;
  .
map:classification_subClassOf a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:subClassOf;
  d2rq:propertyValue owl:Thing;
  .
map:classification__label a d2rq:PropertyBridge;
  d2rq:belongsToClassMap map:tabClassificazioni;
  d2rq:property rdfs:label;
  d2rq:join "tabClassificazioni.NS = tabSistematici.CodiceS";
  d2rq:column "tabSistematici.Descrizione";
  d2rq:condition "tabClassificazioni.ND IS NULL";
  .
map:classification__label_2 a d2rq:PropertyBridge;
  d2rq:belongsToClassMap map:tabClassificazioni;
  d2rq:property rdfs:label;
  d2rq:join "tabClassificazioni.NS = tabSistematici.CodiceS";
  d2rq:join "tabClassificazioni.ND = tabDescrittori.CodiceD";
  d2rq:sqlExpression "CONCAT(tabSistematici.Descrizione, CONCAT(': ', tabDescrittori.Descrizione))";
  d2rq:condition "tabClassificazioni.ND IS NOT NULL";
  .
map:classification_NS a d2rq:PropertyBridge;
  d2rq:belongsToClassMap map:tabClassificazioni;
  d2rq:property dogi:sistematico;
  d2rq:uriPattern "Sistematico/@@tabClassificazioni.NS@@";
  d2rq:additionalPropertyDefinitionProperty map:sistematico_label;
  d2rq:additionalPropertyDefinitionProperty map:sistematico_comment;
  .
map:sistematico_label a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:label;
  d2rq:propertyValue "Sistematico"@en;
  .
map:sistematico_comment a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:comment;
  d2rq:propertyValue "A descriptor for legal documentation identifying a certain law branch (e.g., Administrative Law, Civil Law, Criminal Law)."@en;
  .
map:classification_ND a d2rq:PropertyBridge;
  d2rq:belongsToClassMap map:tabClassificazioni;
  d2rq:property dogi:descrittore;
  d2rq:uriPattern "Descrittore/@@tabClassificazioni.ND@@";
  d2rq:condition "tabClassificazioni.ND != ''";
  d2rq:additionalPropertyDefinitionProperty map:descrittore_label;
  d2rq:additionalPropertyDefinitionProperty map:descrittore_comment;
  .
map:descrittore_label a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:label;
  d2rq:propertyValue "Descrittore"@en;
  .
map:descrittore_comment a d2rq:AdditionalProperty;
  d2rq:propertyName rdfs:comment;
  d2rq:propertyValue "  "@en;
  .

