### Number of instances for each Class

SELECT ?c (COUNT(?i) AS ?cont) {
  ?i a ?c
} GROUP BY ?c

### Number of articles for each Journal

SELECT ?j ?jl (COUNT(?a) AS ?cont) {
  ?j a bibo:Journal ;
     rdfs:label ?jl .
  ?i dcterms:isPartOf ?j .
  ?a bibo:issue ?i .
} GROUP BY ?j ?jl
ORDER BY DESC(?cont)